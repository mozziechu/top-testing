<!--pages/reporting-details/reporting-details.wxml-->
<view>
  <i-row wx:for="{{items}}" wx:key="key">
    <i-col offset="1" span="22" i-class="class1">
      <i-row i-class="material">
        <i-col style="margin-top:10px" offset="1" span="22" i-class="list">
        <view>
        <view style='font-size:15px;margin-top:10px;'>{{item.title}}</view>
        </view>
        </i-col>
        <i-col offset="1" span="8" i-class="particulars">
        <view style='font-size:12px;margin-top:10px;'>{{item.time}}</view>
        </i-col>
        <i-col  span="14" i-class="particulars">
        <view style='font-size:12px;margin-top:10px;margin-bottom:10px'>{{item.date}}</view>
        </i-col>
         <i-col offset="1" span="8" i-class="particulars">
        <view style='font-size:12px;margin-top:10px;margin-bottom:10px'>{{item.sub}}</view>
        </i-col>
        <i-col span="14" i-class="particulars">
        <view style='font-size:12px;margin-top:10px;margin-bottom:10px'>{{item.submitted}}</view>
        </i-col>
        <i-col offset="1" span="8" i-class="particulars">
        <view style='font-size:12px;margin-top:10px;margin-bottom:10px'>{{item.read}}</view>
        </i-col>
        <i-col  span="14" i-class="particulars">
        <view style='font-size:12px;margin-top:10px;margin-bottom:10px'>{{item.toberead}}</view>
        </i-col>
      </i-row>
    </i-col>
  </i-row>
</view>
