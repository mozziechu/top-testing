<!--pages/student-application/student-application.wxml-->
<view>
  <i-row i-class="class1" wx:for="{{items}}" wx:key="key">
    <i-col span="24" i-class="col-class">
      <view>{{item.title}}</view>
    </i-col>

    <i-col span="12" i-class="col-class">
      <view>{{item.people}}</view>
    </i-col>

    <i-col span="12" i-class="col-class">
      <view>{{item.name}}</view>
    </i-col>

    <i-col span="12" i-class="col-class">
      <view>{{item.time}}</view>
    </i-col>

    <i-col span="12" i-class="col-class">
      <view>{{item.date}}</view>
    </i-col>

    <i-col span="12" i-class="col-class">
      <view>{{item.state}}</view>
    </i-col>

    <i-col span="12" i-class="col-class">
      <view>{{item.approval}}</view>
    </i-col>
  </i-row>
</view>
